  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/templates",
        "type": "drupal-library",
        "version": "4.11.1",
        "dist": {
          "type": "zip",
          "url": "https://void-factory.s3.amazonaws.com/Drupal8/Composer/templates_4.11.1.zip",
          "reference": "master"
        }
      }
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "vendor/bin/cghooks add --ignore-lock || echo 'cghooks not found'"
    ],
    "post-update-cmd": [
      "vendor/bin/cghooks update || echo 'cghooks not found'"
    ]
  },
  "config": {
    "disable-tls": true,
    "secure-http": false,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
