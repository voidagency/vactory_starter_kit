        "jQuery color picker remove vendor dependencies": "https://gist.githubusercontent.com/khalidbouhouch77/c128e712544f303f0214857cb362c3c7/raw/bd6219096734da124dc2b7ed95e9e34bfff3122e/jquery-colorpicker-remove-vendor-dependencies.patch",
        "jQuery color picker update background (drupalSettings variable)": "https://gist.githubusercontent.com/khalidbouhouch77/bf02c4bc65240acea5b87a2ff5b5d719/raw/973efb8f4a7bdecd71f21e198de924e98feab66d/jquery-colorpicker-update-background.patch"
      },
      "drupal/domain": {
        "Domain no context : resolve null given.": "https://www.drupal.org/files/issues/2021-03-05/3201514-domain-no-context-test_7.patch"
      },
      "drupal/easy_install": {
        "optional details": "https://www.drupal.org/files/issues/2020-07-25/Solution.patch"
      },
      "drupal/webform": {
        "Invoke alter hook after webform handlers execution": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/webform-post-handlers-hook.patch"
      },
      "drupal/email_registration": {
        "Allow authentication via mail and password over OAuth2 module": "https://www.drupal.org/files/issues/2021-10-06/2991141-19-allow_authentication_via_mail.patch"
      },
      "drupal/simple_oauth": {
        "Auth revoke on profile update": "https://www.drupal.org/files/issues/2019-04-15/simple_oauth-auth-revoke-2946882-34.patch"
      },
      "drupal/captcha": {
        "Fix captcha error message translation": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/f66e3eaaa67a0d0e1de3d55aec61ca327b192d4a/fix-captcha-error-message-translation.patch"
      },
      "drupal/jsonapi_extra": {
        "Default field enhancer for fields": "https://gist.githubusercontent.com/voidagency/4861a0f0343a45cf0582553fc80c6b59/raw/2342d707c2b6708f2c18a69b870fcad1d5242df3/jsonapi_default_field_enhancer_for_fields.patch"
      },
      "drupal/cloudinary": {
        "Cloudinary video media library compatibility fix": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/f8b1871b3e6dac919f09f96b44176a816d8bb4a2/cloudinary-video-media-library-compatibility-fix.patch"
      },
      "drupal/amp": {
        "Fix deprecated php 8.1 issue rtrim": "https://www.drupal.org/files/issues/2022-11-02/amp-stristr-warning-patch.diff"
      },
      "drupal/flag": {
        "Fix deprecated php 8.1 str_replace": "https://www.drupal.org/files/issues/2022-02-25/3266574-2.patch"
      },
      "drupal/paragraphs": {
        "Core 9.5.0 causing broken-looking styling (Legacy widget, with Seven)": "https://www.drupal.org/files/issues/2023-01-06/core-absolute-position-affecting-paragraphs-3331203-3.patch"
      },
      "drupal/jsonapi_search_api": {
        "Modifying a facet does not invalidate caches": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9d8767dd9f024935e94290e89473f2e67bebf64a/fix-jsonapi-search-api-facets-cache-3176987.patch"
      },
      "drupal/lang_dropdown": {
        "Fix TypeError: array_keys(): Argument #1 ($array) must be of type array": "https://www.drupal.org/files/issues/2023-03-16/lang_dropdown-2-0-0--3348277-16.patch"
      }
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/templates",
        "type": "drupal-library",
        "version": "4.11.1",
        "dist": {
          "type": "zip",
          "url": "https://void-factory.s3.amazonaws.com/Drupal8/Composer/templates_4.11.1.zip",
          "reference": "master"
        }
      }
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "vendor/bin/cghooks add --ignore-lock || echo 'cghooks not found'"
    ],
    "post-update-cmd": [
      "vendor/bin/cghooks update || echo 'cghooks not found'"
    ]
  },
  "config": {
    "disable-tls": true,
    "secure-http": false
  }
}
