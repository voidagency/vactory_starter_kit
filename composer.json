  "extra": {
    "hooks": {
      "pre-commit": [
        "source scripts/githooks/pre_commit.sh"
      ],
      "commit-msg": "source scripts/githooks/commit_message.sh",
      "post-merge": "source scripts/githooks/post_merge.sh",
      "pre-push": []
    },
    "php-composter-phpcs-drupal": {
      "standard": "DrupalPractice"
    },
    "php-composter-regular-expression": {
      "commit-message": {
        "subject": {
          "has": "^(Add|Fix|Change|Update|Documentation):|^Merged|^merged"
        }
      }
    },
    "enable-patching": true,
    "patches": {
