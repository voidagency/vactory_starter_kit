    "void/php-composter-phpcs-drupal": "dev-master",
    "void/php-composter-regular-expression": "dev-master"
  },
  "extra": {
    "php-composter-phpcs-drupal": {
      "standard": "DrupalPractice"
    },
    "php-composter-regular-expression": {
      "commit-message": {
        "subject": {
          "has": "^(Add|Fix|Change|Update|Documentation):|^Merged|^merged"
        }
      }
    },
    "enable-patching": true,
    "patches": {
      "drupal/simplify_menu": {
        "Add support for options.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/fd5550d45f742ac17fd6d833ef33c0e02e73a57d/simplify_menu-support-options.patch"
      },
      "drupal/features": {
        "Export config translation": "https://www.drupal.org/files/issues/features_export-config-translation-2869336-2.patch"
      },
      "drupal/core": {
        "Add internal path check to blocks visibility pages evaluation.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/19e54a007837537dd7de97456ab0e1ad0b9d3cf6/block-visibility-pages-paths-evaluate.patch",
        "Composer 2: Fatal error Call to undefined method getJobType()": "https://www.drupal.org/files/issues/2020-11-04/drupal-composer2-3162228.patch",
        "fix undefined offset on date form element": "https://www.drupal.org/files/issues/2021-04-07/3189302_11.patch"
      },
      "drupal/rate": {
        "Make rate available for all node display mode.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/19e54a007837537dd7de97456ab0e1ad0b9d3cf6/rate-add-rate-to-all-node-display-mode.patch",
      },
      "drupal/oauth_login_oauth2": {
        "Store ressourceOwner into a session variable.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/d86aeeb1921f10cc36198efb5f4ac00504e9c2ee/oauth-login-oauth2-add-resource-owner-to-session.patch"
      },
      "drupal/social_auth_microsoft": {
        "Fixing call to function on null.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/19e54a007837537dd7de97456ab0e1ad0b9d3cf6/microsoft_social_auth.patch",
        "Update to v2 endpoints.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/social_auth_microsoft-v2.patch"
      },
      "drupal/focal_point": {
        "Add the focal point to image.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/focal-point-add-patch.patch"
      },
      "stevenmaguire/oauth2-microsoft": {
        "Fix Bearer Authorization - Update ResourceOwner": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/oauth2-microsoft-authorization-ressource-owner.patch"
      },
      "drupal/media_library_form_element": {
        "Fixing multiple modals closing": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/locator_media_library_images.patch"
      },
      "drupal/viewsreference": {
        "Exposed filters not working for Views in Paragraphs": "https://www.drupal.org/files/issues/2020-05-13/viewsreference-exposed_filters_not_working-3129609-3.patch"
      },
      "drupal/swiftmailer": {
        "Fix undefined index warnings on reset password form": "https://www.drupal.org/files/issues/2020-10-29/3179656-two-undentified-index-notices-2.patch",
        "Swiftmailer fix renderPlan method cli breaks": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/e11467100b3dd3c3ee66026b5f6ee781712b214c/swiftmailer-prevent-cli-commands-break.patch"
      },
      "drupal/advagg": {
        "Remove version comments from minified files.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/advagg-remove-version-comment-from-minified-js.patch"
      },
      "drupal/colorapi": {
        "Color api : Fix undefined index name.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/cd98ab758217f762b1f8daeda8774f19cea1f9dc/fix-color-api-undefined-index-name.patch",
        "Enable colorapi field type by default": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/colorapi-enable-colorapi-field-type-by-default.patch"
      },
      "drupal/migrate_tools": {
        "Fix migrate tools drush command not found": "https://www.drupal.org/files/issues/2019-12-11/3099611-15.patch"
      },
      "drupal/jquery_colorpicker": {
        "jQuery color picker resolve vendor path": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/d6f8848db0da2b9c79d6860c81a39831dce9ed18/jquery-color-picker-resolve-vendor-path.patch"
      },
      "drupal/domain": {
        "Domain no context : resolve null given.": "https://www.drupal.org/files/issues/2021-03-05/3201514-domain-no-context-test_7.patch"
      },
      "drupal/easy_install": {
        "optional details": "https://www.drupal.org/files/issues/2020-07-25/Solution.patch"
      },
      "drupal/nocurrent_pass": {
        "Settings are not applied if use not a standard path": "https://www.drupal.org/files/issues/2018-10-09/user-from-session-3005310-2.patch"
      },
      "drupal/webform": {
        "Invoke alter hook after webform handlers execution": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/9e6eca9bcbd95e7bed7859d54a0c4623a6112e5a/webform-post-handlers-hook.patch"
      }
    }
  },
