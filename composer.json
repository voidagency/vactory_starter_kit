      },
      "drupal/features": {
        "Export config translation": "https://www.drupal.org/files/issues/features_export-config-translation-2869336-2.patch"
      },
      "drupal/oauth_login_oauth2": {
        "Store ressourceOwner into a session variable.": "https://gist.githubusercontent.com/b-khouy/d796ede15da1e0b69fa22af52ad55b04/raw/d86aeeb1921f10cc36198efb5f4ac00504e9c2ee/oauth-login-oauth2-add-resource-owner-to-session.patch"
